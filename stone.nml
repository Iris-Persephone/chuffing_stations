}

item(FEAT_STATIONS, stone_plain_platform_left_station) {
    property {
        class:                  "STNE";
        classname:              string(STR_NAME_STATCLASS_STONE_PLATFORMS);
        name:                   string(STR_NAME_STATION_PLATFORM);
        cargo_threshold:        160;
