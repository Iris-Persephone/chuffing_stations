        yextent:        16;
        zextent:        12;
        recolour_mode:  RECOLOUR_REMAP;
        palette:        PALETTE_USE_DEFAULT;
    }
}

item(FEAT_STATIONS, darkbrick_plain_platform_left_station) {
    property {
        class:                  "DARK";
        classname:              string(STR_NAME_STATCLASS_DARKBRICK_PLATFORMS);
        name:                   string(STR_NAME_STATION_PLATFORM);
        cargo_threshold:        160;
    }
    graphics {
        sprite_layouts: [darkbrick_plain_platform_left_sw_ne(0), darkbrick_plain_platform_left_se_nw(0)];
